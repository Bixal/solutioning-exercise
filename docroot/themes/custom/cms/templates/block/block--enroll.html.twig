{% set classes = [
  'block',
  'ds-u-margin-top--2',
] %}

{% embed '@components/block/block.twig' with {
  'label': label is not empty ? label : false,
  'title_prefix': title_prefix,
  'title_suffix': title_suffix,
  'hide_content_wrapper': true
} %}

  {% block content %}
    {% include '@components/enroll/enroll.twig' with {
      'heading': content.field_heading|field_value,
      'teaser': content.field_teaser|field_value,
      'steps': content.field_steps|field_value
    } %}
  {% endblock %}

{% endembed %}
