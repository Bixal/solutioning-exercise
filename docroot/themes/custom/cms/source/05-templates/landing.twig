{% embed '@layouts/sidebar/sidebar.twig' with {
  'header': true,
  'aside': true
} %}

  {% block header %}
    {% include '@components/page-title/page-title.twig' with {
      'page_title': landing_title
    } %}
    {{ landing_header }}
  {% endblock %}

  {% block main %}
    {{ landing_content }}
  {% endblock %}

  {% block aside %}
    {{ landing_aside }}
  {% endblock %}

{% endembed %}